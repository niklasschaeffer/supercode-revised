{
  "name": "supercode-landingpage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "build:dev": "tsc && vite build --mode development",
    "preview": "vite preview --port 4173",
    "preview:build": "npm run build && npm run preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf ../docs/*",
    "verify": "./scripts/verify-deployment.sh",
    "deploy": "npm run verify && git add ../docs/ && git commit -m 'Deploy to GitHub Pages' && git push origin main"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "tailwind-merge": "^2.0.0",
    "vite-plugin-generate-file": "^0.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.24.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
